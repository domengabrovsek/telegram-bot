{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index",
    "lint": "eslint . --ext .ts --config package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domengabrovsek/telegram-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/domengabrovsek/telegram-bot/issues"
  },
  "homepage": "https://github.com/domengabrovsek/telegram-bot#readme",
  "dependencies": {
    "fastify": "^4.12.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.1",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "no-console": "off"
    }
  }
}
